        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023</dcterms:issued> <!-- Date of the source document, comment out if draft -->
        <owl:priorVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-11-05</owl:priorVersion> <!-- Date of prior ontology file, if any -->
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2026-01-13</dcterms:modified> <!-- Date of this ontology file -->
        <owl:versionInfo>1.3.0</owl:versionInfo>
        <owl:versionIRI rdf:resource="https://w3id.org/isoiec5087/-1/v1/r3.0"/>
        <rdfs:comment>The prior version of this ontology was divided into a distinct file 
            for each pattern contained within Part 1; this version combines all of the 
            patterns into a single file for ease of reference. Some of the prior component 
