    <!-- Issue: The class Acceleration is not defined in ISO/IEC 5087-1. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/Acceleration -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Acceleration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/AccelerationUnit"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>acceleration</rdfs:label>
    </owl:Class>
    

    <!-- Issue: The class AccelerationUnit is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/AccelerationUnit -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/AccelerationUnit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Unit_of_measure"/>
        <rdfs:label>acceleration unit</rdfs:label>
    </owl:Class>
    

    <!-- Issue: The class AmountOfMoney is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/AmountOfMoney -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/AmountOfMoney">
