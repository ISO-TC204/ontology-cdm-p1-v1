    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/Area -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Area">
        <skos:definition>Area expresses the two-dimensional size of a defined part of a surface, typically a region bounded by a closed curve. It is a derived quantity in the International System of Units. Area is length squared.</skos:definition>
        <rdfs:label>area</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/AreaUnit"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/AreaUnit -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/AreaUnit">
        <skos:definition>Unit of area in the specified system of units.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Unit_of_measure"/>
        <rdfs:label>area unit</rdfs:label>
    </owl:Class>
    

    <!-- Issue: The class Capacity is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/Capacity -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Capacity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/CityUnitsThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/defined_by"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: The class CapacityRate is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/CapacityRate -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/CapacityRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/CardinalityUnitPerTime"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: i72:cardinality_of is defined with a domain of Cardinality and a range of Population; it is applued -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/cardinality_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087/-1/v1/Capacity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: The class CapacitySize is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/CapacitySize -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/CapacitySize">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec21972/v1/Cardinality_unit"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/cardinality_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087/-1/v1/Capacity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: The class CardinalityMeasure is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/CardinalityMeasure -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/CardinalityMeasure">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Measure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec21972/v1/Cardinality_unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: The class CardinalityUnitPerTime is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/CardinalityUnitPerTime -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/CardinalityUnitPerTime">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Unit_of_measureDivision"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/denominator"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/TimeUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/numerator"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec21972/v1/Cardinality_unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087/-1/v1/Duration -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Duration">
        <skos:definition>Duration is the amount of time that elapses between two instants or events.</skos:definition>
        <rdfs:label>duration</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#TemporalDuration"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/TimeUnit"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: ISO/IEC 5087-1 refers to the concept of "Function" and "function", but these are not defined in the document. This ontology defines a stub for Function with no real meaning. -->    <!-- https://w3id.org/isoiec5087/-1/v1/Function -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Function">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/CityUnitsThing"/>
        <rdfs:label>function</rdfs:label>
    </owl:Class>
    


    <!-- Issue: The class Geo_Position_unit is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/Geo_Position_unit -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Geo_Position_unit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Unit_of_measure"/>
        <rdfs:comment>Reference systems to indicate geographic position.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/Length -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Length">
        <skos:definition>Length is the amount of space between two geographical points along a curve. It is a base quantity in the International System of Units and other systems of units. Length is speed times time. The metre, a base unit of length in the International System of Units, is defined in terms of speed of light during a certain time interval.</skos:definition>
        <rdfs:label>length</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/isoiec21972/v1/Measure"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/unit_of_measure"/>
                                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/LengthUnit"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/LengthUnit -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/LengthUnit">
        <skos:definition>A specified system of units for distance.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Unit_of_measure"/>
        <rdfs:label>length unit</rdfs:label>
    </owl:Class>
    


    <!-- Issue: The class Mass is not defined in ISO/IEC 5087-1 or i72. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/Mass -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Mass">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec21972/v1/value"/>
                <owl:allValuesFrom>
