    <!-- https://w3id.org/isoiec5087/-1/v1/CityUnitsThing -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/CityUnitsThing">
        <rdfs:comment>Added for organizational purposes to group the classes defined in the City Units portion of the CDM.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmThing"/>
