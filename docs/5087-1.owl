    </owl:ObjectProperty>
    

    <!-- Issue: The property hasMonth is not defined in ISO/IEC 5087-1 or i72 and is not used; although it is referenced in textual descriptions in this file but not in the standard. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/hasMonth -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasMonth">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasOccurrence -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasOccurrence">
        <skos:definition>Identifies the Activities that take place at the time and location.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasSubRecurringEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasSubRecurringEvent">
        <skos:definition>identifies the sub-recurring events that comprise the RecurringEvent. As with an Activity, a RecurringEvent may be decomposed/decomposed into simpler/more complex RecurringEvents to support varying levels of granularity.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/onDateTimeDescription -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/onDateTimeDescription">
        <skos:definition>Specifies the date-time description (as defined in the Time pattern) on which a recurring event recurs. For example, the day of the month on which a MonthlyRecurringEvent recurs, or the day and month on which a YearlyRecurringEvent recurs.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>    
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087/-1/v1/recursAddition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/recursAddition">
        <skos:definition>Defines a condition when the RecurringEvent should be added to. This can specify a time, day of the week, or specific dates.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#DayOfWeek"/>
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/recursExcept -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/recursExcept">
        <skos:definition>Defines the exceptions for the RecurringEvent, i.e. conditions when it does not occur. This can specify a time, day of the week, or specific dates.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#DayOfWeek"/>
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/associatedLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/associatedLocation">
        <skos:definition>Captures the association of a non-spatial object with a Location. For example, an activity is not a spatial object but may be associated with some location in which it (mostly) takes place.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/LocationObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Location"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <!-- https://w3id.org/isoiec5087/-1/v1/endTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/endTime">
        <skos:definition>Specifies the end time of the domain object's activity.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>    
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/isoiec5087/-1/v1/startTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/startTime">
        <skos:definition>Specifies the start time of the RecurringEvent's activity using http://www.w3.org/2001/XMLSchema#time format.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>    
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087/-1/v1/DailyRecurringEvent -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/DailyRecurringEvent">
        <skos:definition>A DailyRecurringEvent is a RecurringEvent that occurs every day. It has a maximum of one associated time, the start time. Typically, a daily recurring event will occur at the same time every day, but it is also possible that no commitment is made to a recurring start time for the event, in which case no start time is specified. A DailyEvent does not necessarily have the same endtime or duration, therefore these are not specified.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/startTime"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- Issue: ISO/IEC 5087-1 defines the range of onDateTimeDescription to be time:DateTimeDescription-->
    <!-- https://w3id.org/isoiec5087/-1/v1/ExceptionDay -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/ExceptionDay">
        <skos:definition>An ExceptionDay specifies a day or days that recursExcept and recursAddition use to specify unique days that do not recur on the same day each year, for example, holidays. </skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/onDateTimeDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2006/time#GeneralDateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: ISO/IEC 5087-1 defines the range of onDateTimeDescription to be more restrictive -->
    <!-- https://w3id.org/isoiec5087/-1/v1/MonthlyRecurringEvent -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/MonthlyRecurringEvent">
        <skos:definition>A MonthlyRecurringEvent recurs regularly on the same day of each month, as specified by the dayOfMonth data property. Note that there is often ambiguity regarding the semantics of a monthly recurring event: in this formalization, a MonthlyRecurringEvent is any event that recurs regularly on the same day of each month; other interpretations sometimes consider events that recur on the same day of week, or first or last day, in which case the day of month will vary.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/onDateTimeDescription"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#GeneralDateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- Issue: ISO/IEC 5087-1 includes a dayOfWeek property rather than a hasDayOfWeek property; according to the standard, the latter is limited to WeeklyRecurringEvent. It should likely be time:dayOfWeek.-->
    <!-- https://w3id.org/isoiec5087/-1/v1/RecurringEvent -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/RecurringEvent">
        <skos:definition>Recurring events are defined based on the regular interval at which they occur; this is captures using some combination of the hasTime, dayOfWeek, hasMonth, and dayOfMonth properties.  Using these properties, ontology defines the following specializations of the RecurringEvent class. Other subclasses may be defined similarly, as required.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEventThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/beginsRecurringState"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/beginsRecurringTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/endsRecurringState"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/endsRecurringTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasDayOfWeek"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#DayOfWeek"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasOccurrence"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasSubRecurringEvent"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/onDateTimeDescription"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#GeneralDateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/recursAddition"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DayOfWeek"/>
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/recursExcept"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DayOfWeek"/>
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/associatedLocation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/endTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/startTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/RecurringEventThing -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/RecurringEventThing">
        <rdfs:comment>Class added for organizational purposes only; to group all classes defined in the recurring-event ontology.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmThing"/>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/WeeklyRecurringEvent -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/WeeklyRecurringEvent">
        <skos:definition>A WeeklyRecurringEvent recurs on the regularly on the same day of the week, as specified by the schema:dayOfWeek property.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasDayOfWeek"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DayOfWeek"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- Issue: ISO/IEC 5087-1 defines the range of onDateTimeDescription to be more restrictive -->
    <!-- https://w3id.org/isoiec5087/-1/v1/YearlyRecurringEvent -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/YearlyRecurringEvent">
        <skos:definition>A YearlyRecurringEvent recurs regularly on the same day of the same month, as specified by the hasMonth and dayOfMonth properties. As with MonthlyRecurringEvent, there may be ambiguity regarding the semantics of a yearly recurring event, however this formalization captures only the notion of an event that recurs on the same day of the same month (e.g. a birthday).</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/RecurringEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/onDateTimeDescription"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#GeneralDateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/Location -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Location">
        <skos:definition>A specialization (subclass) of geo:Geometry that refers to a geospatial representation of the space occupied by some object.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmThing"/>
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
    </owl:Class>
    



    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/isoiec5087/-1/v1/capacityInUse -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/capacityInUse">
        <skos:definition>Identifies the portion of capacity in use, by some Activity(s).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>    
    </owl:ObjectProperty>
    

    <!-- Issue: The property consumes is not defined in ISO/IEC 5087-1 -->
    <!-- https://w3id.org/isoiec5087/-1/v1/consumedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/consumedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/preconditionOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/forResource -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/forResource">
        <skos:definition>Specifies the Resource that is allocated.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/PlannedAllocation"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>    
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/forState -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/forState">
        <skos:definition>Specifies the State to which the Resource is allocated.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/PlannedAllocation"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>  
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasAllocation"/>  
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasAllocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasAllocation">
        <skos:definition>Specifies a Planned Allocation for the Resource. This indicates a State and time Interval to which the Resource has been allocated.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- Issue: In ISO/IEC 5087-1, hasAsset is listed in a table as a null entry with no other reference and this file does not provide any additional information -->
    <!-- https://w3id.org/isoiec5087/-1/v1/hasAsset -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasAsset">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasAvailableCapacity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasAvailableCapacity">
        <skos:definition>identifies the Quantity that specifies how much of the Resource is available for use.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>    
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasCapacity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasCapacity">
        <skos:definition>Identifies the Quantity that specifies how much of the Resource exists, e.g. its volume, if it is liquid; how much it can hold, if it is an container.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>    
    </owl:ObjectProperty>
    

    <!-- Issue: hasOwner is not defined in ISO/IEC 5087-1. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/hasOwner -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasOwner">
        <skos:definition>A Resource may have some owner. The owner of a resource is subject to change over time.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ownedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087/-1/v1/hasQuantity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasQuantity">
        <skos:definition>Specifies the Quantity of the Resource that participates in the State, if applicable.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/PlannedAllocation"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>    
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasResource -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasResource">
        <skos:definition>Specifies a Resource that participates in the State. This identifies the condition of the object that plays a resource role (i.e. is used, consumed, etc.) when the state’s status is active.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/resourceOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/TerminalResourceState"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>    
    </owl:ObjectProperty>
    

    <!-- Issue: ownedBy is not defined in ISO/IEC 5087-1. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/ownedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/ownedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/participatesIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/participatesIn">
        <skos:definition>Identifies the Activity that the Resource is being used, consumed, produced, or released by.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>    
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/resourceOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/resourceOf">
        <skos:definition>identifies what State the agent may be a resource of. This is specified using the Resource pattern, where the Agent would be considered to be a resource from the perspective of an Activity that uses it in some way. For example, a construction activity may use some persons(s) and those persons would be considered a Resource for that Activity.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- Issue: usedBy is not defined in ISO/IEC 5087-1. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/usedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/usedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/preconditionOf"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087/-1/v1/ConsumeState -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/ConsumeState">
        <skos:definition>Identifies a Resource and Quantity it consumes. The Quantity is removed from the Resource.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/TerminalResourceState"/>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/DivisibleResource -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/DivisibleResource">
        <skos:definition>A Resource that can be divided for use or consumption between multiple Activities.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/NonDivisibleResource -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/NonDivisibleResource">
        <skos:definition>A Resource that can only be used for a single Activity at once - even if it isn't fully utilized.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
    </owl:Class>
    

    <!-- Issue: PlannedAllocation is not defined to be of type Manifestation in ISO/IEC 5087-1. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/PlannedAllocation -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/PlannedAllocation">
        <skos:definition xml:lang="en">Specifies the planned allocation of a resource to an activity via a state.
        Note that Allocation is a Manifestation as the allocation may change over time.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/forResource"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/forState"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasQuantity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/ProduceState -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/ProduceState">
        <skos:definition>Identifies a Resource and Quantity it produces.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/TerminalResourceState"/>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/ReleaseState -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/ReleaseState">
        <skos:definition>Identifies a Resource and Quantity it releases (after using).</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/TerminalResourceState"/>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/Resource -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/Resource">
        <skos:definition>A Resource is a generic representation of some Thing that can be &quot;used&quot; in an Activity. 
        A Resource may have some Location, amount or availability, according to the definition of the Manifestation or TimeVaryingEntity. 
        A Resource must be classified as some Resource Type.
        A Resource may participate in some Activity Occurrence. In other words, it links to the Activity that the Resource is being used/consumed/produced/released at the time of the manifestation.
        A specific Resource may be used in or consumed in some activity occurrence.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasLocation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasAllocation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/PlannedAllocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/participatesIn"/>
                <!-- Issue: Resource.participatesIn is defined as only Activity in ISO/IEC 5087-1 rather than min 1 -->
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/capacityInUse"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasAvailableCapacity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasCapacity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/DivisibleResource"/>
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/NonDivisibleResource"/>
        </owl:disjointUnionOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/ResourceThing -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/ResourceThing">
        <skos:definition>Added for organizational purposes, to identify all classes defined in the Resource ontology.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmThing"/>
    </owl:Class>
    

    <!-- Issue: No definition provided for ResourceState -->
    <!-- https://w3id.org/isoiec5087/-1/v1/TerminalResourceState -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/TerminalResourceState">
        <skos:definition></skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/TerminalState"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ResourceThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasAllocation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/PlannedAllocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasQuantity"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasResource"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/UseState -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/UseState">
        <skos:definition>Identifies a Resource and Quantity it uses (without consuming).</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/TerminalResourceState"/>
    </owl:Class>

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087/-1/v1/hasLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasLocation">
        <skos:definition>Specifies the Location of an object.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/LocationObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Location"/>
    </owl:ObjectProperty>
    

    <!-- Issue: No definition provided for nonTangentialProperPart -->
    <!-- https://w3id.org/isoiec5087/-1/v1/nonTangentialProperPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/nonTangentialProperPart">
        <rdfs:subPropertyOf rdf:resource="http://www.opengis.net/ont/geosparql#rcc8ntpp"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/properPartOf"/>
    </owl:ObjectProperty>
    

    <!-- Issue: No definition provided for tangentialProperPart -->
    <!-- https://w3id.org/isoiec5087/-1/v1/tangentialProperPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/tangentialProperPart">
        <rdfs:subPropertyOf rdf:resource="http://www.opengis.net/ont/geosparql#rcc8tpp"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/properPartOf"/>
    </owl:ObjectProperty>
    
    <!-- Issue: as_nDLatLon is not defined in ISO/IEC 5087-1 -->
    <!-- https://w3id.org/isoiec5087/-1/v1/as_nDLatLon -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/as_nDLatLon">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasSerialization"/>
        <rdfs:domain rdf:resource="http://www.opengis.net/ont/sf#Geometry"/>
        <rdfs:range rdf:resource="http://franz.com/ns/allegrograph/5.0/geo/nd#_lat_la_-9.+1_+9.+1_+1.-4_+1.-1_lon_lo_-1.8+2_+1.8+2_+1.-4"/>
    </owl:DatatypeProperty>
    

    <!-- Issue: hasSerialization is not defined in ISO/IEC 5087-1 -->
    <!-- https://w3id.org/isoiec5087/-1/v1/hasSerialization -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasSerialization">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/SpatialLocDataProperty"/>
    </owl:DatatypeProperty>
    
</rdf:RDF>


    <!-- https://w3id.org/citydata/test/isInvariant -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/citydata/test/isInvariant">
        <rdfs:comment xml:lang="en">Identifies whether a property is to be interpreted as invariant (i.e., its value is not subject to change over time).</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:AnnotationProperty>
    
