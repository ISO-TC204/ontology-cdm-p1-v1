    <!-- https://w3id.org/isoiec5087/-1/v1/hasRelativeYear -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasRelativeYear">
        <skos:definition>The base year used to define monetary values for a currency.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/CityUnitsDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
    </owl:DatatypeProperty>
    

